{
    "name" : "tomatom/atom-bundle",
    "description" : "Symfony bundle",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Tom Atom",
        "email" : "tom@tomatom.com"
    }],
    "keywords" : [
        "symfony atom bundle front-end editor"
    ],
    "license" : "MIT",
    "require" : {
        "php": ">=8.1",
        "doctrine/orm": "^2",
        "gedmo/doctrine-extensions": "^3",
        "knplabs/doctrine-behaviors": "^2",
        "symfony/framework-bundle": "^6",
        "symfony/security-bundle": "^6",
        "symfony/twig-bundle": "^6",
        "symfony/http-client": "^7",
        "twig/cache-extra": "^3",
        "deeplcom/deepl-php": "^1.9",
        "nyholm/psr7": "^1.8"
    },
    "autoload" : {
        "psr-4" : {
            "TomAtom\\AtomBundle\\": "src/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        },
        "public-dir": "public"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
