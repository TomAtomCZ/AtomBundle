{
    "name" : "tomatom/atom-bundle",
    "description" : "Symfony bundle",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Tom Atom",
        "email" : "tom@tomatom.com"
    }],
    "keywords" : [
        "atom bundle"
    ],
    "license" : "MIT",
    "require" : {
        "php": ">=8.1",
        "knplabs/doctrine-behaviors": "^2.6",
        "symfony/framework-bundle": "^6",
        "symfony/security-bundle": "^6",
        "twig/cache-extra": "^3.3",
        "gedmo/doctrine-extensions": "^3.17",
        "doctrine/orm": "^2.20"
    },
    "autoload" : {
        "psr-4" : {
            "TomAtom\\AtomBundle\\": "src/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        },
        "public-dir": "public"
    }
}
